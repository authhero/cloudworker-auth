{
  "name": "auth2",
  "version": "1.257.8",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "bun  --watch src/bun.ts",
    "dev:wrangler": "wrangler dev --remote",
    "dev:https": "wrangler dev --remote --local-protocol=https",
    "format": "prettier --write '**/*.{ts,css,json,md}'",
    "format:ci-cd": "prettier --check '**/*.{ts,css,json,md}'",
    "lint": "eslint",
    "test": "vitest run --pool=forks",
    "test:watch": "vitest --pool=forks",
    "test:snapshots": "TEST_SNAPSHOTS=true vitest --pool=forks",
    "test:update-snapshots": "TEST_SNAPSHOTS=true vitest --pool=forks -u",
    "semantic-release": "semantic-release",
    "type-check": "tsc",
    "pull-translations": "i18nexus pull -k U5P1_tq3Teu7FZx4OrfI9w --path ./src/localesLogin2 && prettier --write ./src/localesLogin2/**/*.json && yarn sort-translations",
    "knip": "knip",
    "postcss": "postcss ./src/styles/input.css -o ./src/styles/tailwind.css",
    "sort-translations": "sort-json src/localesLogin2/*/*.json",
    "docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.44.1-focal /bin/bash"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@authhero/adapter-interfaces": "0.21.0",
    "@authhero/kysely-adapter": "0.17.1",
    "@hono/zod-openapi": "0.16.2",
    "@peculiar/x509": "^1.12.2",
    "@planetscale/database": "1.19.0",
    "@xmldom/xmldom": "^0.9.2",
    "arctic": "^1.9.2",
    "bcryptjs": "^2.4.3",
    "fast-xml-parser": "^4.5.0",
    "hono": "4.4.0",
    "kysely": "^0.27.4",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.5.0",
    "liquidjs": "^10.9.2",
    "lru-cache": "^11.0.1",
    "nanoid": "5.0.7",
    "oslo": "^1.2.1",
    "playwright": "^1.44.1",
    "xml-crypto": "^6.0.0",
    "xmldsigjs": "^2.6.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@ape-egg/tailwind-rows-columns": "1.0.2",
    "@cloudflare/workers-types": "4.20240925.0",
    "@semantic-release/git": "10.0.1",
    "@types/bcryptjs": "2.4.6",
    "@types/better-sqlite3": "7.6.11",
    "@types/cookie": "0.6.0",
    "@types/jest-image-snapshot": "6.4.0",
    "@types/node": "20.12.12",
    "@types/pako": "^2.0.3",
    "@types/service-worker-mock": "2.0.4",
    "@types/validator": "13.11.10",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "11.3.0",
    "classnames": "^2.5.1",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "husky": "9.1.6",
    "i18next": "23.11.4",
    "i18nexus-cli": "3.4.0",
    "jest-image-snapshot": "6.4.0",
    "knip": "5.16.0",
    "mjml": "4.15.3",
    "msw": "^2.3.0",
    "postcss-cli": "^11.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "semantic-release": "23.0.8",
    "sort-json": "2.0.1",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2",
    "validator": "13.12.0",
    "vitest": "2.1.3",
    "vitest-fetch-mock": "0.3.0",
    "wrangler": "3.81.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
